{
  "kind": "collectionType",
  "collectionName": "casinos",
  "info": {
    "singularName": "casino",
    "pluralName": "casinos",
    "displayName": "Casino",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "website": {
      "type": "string"
    },
    "MinPayout": {
      "type": "float"
    },
    "currencies": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::currency.currency",
      "mappedBy": "casino"
    },
    "partner": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::partner.partner",
      "inversedBy": "casinos"
    },
    "invoices": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::invoice.invoice",
      "mappedBy": "casino"
    },
    "logo": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "shortName": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "Tag": {
      "type": "component",
      "repeatable": true,
      "component": "csino-tags.tag"
    },
    "status": {
      "type": "enumeration",
      "enum": [
        "Active",
        "Closed",
        "Blacklisted",
        "Restricted",
        "Upcoming"
      ],
      "default": "Active",
      "required": true
    },
    "affiliate_sites": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::affiliate-site.affiliate-site",
      "inversedBy": "casinos",
      "required": true
    }
  }
}
